
<div class="jumbotron">
  <h1 class="display-4"><%= @shop.shop_name %>-</h1>
  <p class="lead"><%= @shop.location %> | <%= @shop.email %> | <%= @shop.phone %>  </p>
  <br/>
  <%= link_to 'Edit', edit_shop_path(@shop), class:"btn btn-dark" %>
  <%= link_to 'Delete', @shop, method: :delete, data: { confirm: 'Are you sure?' }, class:"btn btn-danger" %> 
<%= link_to 'Back', shops_path, class:"btn btn-secondary" %>
</div>





